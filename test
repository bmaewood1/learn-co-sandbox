def play(songs)
  puts "Please enter a song name or number:"
  input = gets.strip
  if input.length > 1 && songs[input] != nil
    puts "Playing #{input}"
  elsif input.length = 1 && input.to_i != nil && input.to_i >= 0 && input.to_i <= songs.length
    input2 = input.to_i - 1
    puts "Playing #{songs[input2]}"
  else
    puts "Invalid command, please try again"
  end
end

play(songs)