def play(songs)
  puts "Please enter a song name or number:"
  input = gets.strip
  input2 = input.to_i
  if input2 < songs.length && input2 >= 0
    input3 = input2 - 1
    puts "Playing #{songs[input3]}"
  elsif songs.index(input) != nil
    puts "Playing #{input}"
  else
    puts "Invalid input, please try again"
  end
end

